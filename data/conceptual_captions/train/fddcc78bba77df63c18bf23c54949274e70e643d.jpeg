    function addPageSponsor(posttype,product_id,sponsor_id){
        //$(".add-new-post").toggle();
        //tinyMCE.remove();
        $("#my-posts-loader").css("display","block");
        $.ajax({
            url:'https://awe365.com/wp-content/themes/awenewtheme/ajax-add-page-sponsor.php',   
            type:'POST',
            data:{ 'posttype': posttype, 'product_id': product_id, 'sponsor_id': sponsor_id },
            dataType:'html',
            success:function(msg){
                //alert(msg);
                $("#editPosts").html(msg);
                $("#editPosts").css("display","block"); 
                $("#my-posts-loader").css("display","none");
            } 
        });
    }
    function editPageSponsor(id,posttype){
        $("#my-posts-loader").css("display","block");
        $.ajax({
            url:'https://awe365.com/wp-content/themes/awenewtheme/ajax-edit-page-sponsor.php',   
            type:'POST',
            data:{ 'id': id, 'posttype': posttype },
            dataType:'html',
            success:function(msg){
                //alert(msg);
                $("#editPosts").html(msg);
                $("#editPosts").css("display","block"); 
                $("#my-posts-loader").css("display","none");
            } 
        });
    }
    
    function showCompanyForm(posttype){
        //$(".add-new-post").toggle();
        //tinyMCE.remove();
        $("#my-posts-loader").css("display","block");
        $.ajax({
            url:'https://awe365.com/wp-content/themes/awenewtheme/ajax-add-company.php',   
            type:'POST',
            data:{ 'posttype': posttype },
            dataType:'html',
            success:function(msg){
                //alert(msg);
                $("#editPosts").html(msg);
                $("#editPosts").css("display","block"); 
                $("#my-posts-loader").css("display","none");
                $('html, body').animate({
                    scrollTop: parseInt($("#companyForm").offset().top)
                }, 2000);
            } 
        });
    }
    function editCompanyPost(id,posttype){
        //alert(posttype)
        //tinyMCE.remove();
        $("#my-posts-loader").css("display","block");
        $.ajax({
            url:'https://awe365.com/wp-content/themes/awenewtheme/ajax-edit-company.php',   
            type:'POST',
            data:{ 'id': id, 'posttype': posttype },
            dataType:'html',
            success:function(msg){
                //alert(msg);
                $("#editPosts").html(msg);
                $("#editPosts").css("display","block"); 
                $("#my-posts-loader").css("display","none");
                $('html, body').animate({
                    scrollTop: parseInt($("#holidayForm").offset().top)
                }, 2000);
            } 
        });
    }
    
    function showHolidayForm(posttype){
        //$(".add-new-post").toggle();
        //tinyMCE.remove();
        if(posttype == "discount"){
            var pagename = 'ajax-add-discount.php';
        }else{
            var pagename = 'ajax-add-holiday.php';
        }
        $("#my-posts-loader").css("display","block");
        $.ajax({
            url:'https://awe365.com/wp-content/themes/awenewtheme/'+pagename,   
            type:'POST',
            data:{ 'posttype': posttype },
            dataType:'html',
            success:function(msg){
                //alert(msg);
                $("#editPosts").html(msg);
                $("#editPosts").css("display","block"); 
                $("#my-posts-loader").css("display","none");
                $('html, body').animate({
                    scrollTop: parseInt($("#holidayForm").offset().top)
                }, 2000);
            } 
        });
    }
    function editMyPost(id,posttype){
        //alert(posttype)
        //tinyMCE.remove();
        if(posttype == "discount"){
            var pagename = 'ajax-edit-discount.php';
        }else{
            var pagename = 'ajax-edit-holiday.php';
        }
        $("#my-posts-loader").css("display","block");
        $.ajax({
            url:'https://awe365.com/wp-content/themes/awenewtheme/'+pagename,   
            type:'POST',
            data:{ 'id': id, 'posttype': posttype },
            dataType:'html',
            success:function(msg){
                //alert(msg);
                $("#editPosts").html(msg);
                $("#editPosts").css("display","block"); 
                $("#my-posts-loader").css("display","none");
                $('html, body').animate({
                    scrollTop: parseInt($("#holidayForm").offset().top)-100
                }, 2000);
            } 
        });
    }
    
    function viewEnqDetails(eid,uid,post_id){
        $("#my-posts-loader").css("display","block");
        $.ajax({
            url:'https://awe365.com/wp-content/themes/awenewtheme/ajax-enquiry-detail.php',   
            type:'POST',
            data:{ 'eid':eid, 'uid':uid, 'post_id':post_id },
            dataType:'html',
            success:function(msg){
                $("#my-posts-loader").css("display","none");
                $("#enqDetails").html(msg);
                //alert(msg);
                 $('#viewEnquiry').modal('show');
            } 
        });
    }
    
    function myStatLise(v,mn,py){
        window.location.href = "https://awe365.com/my-account/view-statistics/?stat="+v+"&mn="+mn+"&py="+py;
    }
    
