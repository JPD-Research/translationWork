        [].forEach.call(document.querySelectorAll('.c-menu'), function(el) {
            el.classList.toggle('c-mobile-menu-active');
        });

        [].forEach.call(document.querySelectorAll('.c-hamburger'), function(el) {
            el.classList.toggle('c-is-active');
        });
    });

    window.addEventListener("DOMContentLoaded", ()=>{
        // Add { passive: true } for the touchstart event listener
        window.addEventListener("touchstart", function(e) {}, { passive: true });

        document.querySelectorAll(".c-menu-open").forEach((item)=>{
            item.addEventListener("touchstart", function(e) {
                this.classList.add("hover");
                for (let sibling of item.parentNode.children) {
                    if (sibling !== item) {
                        sibling.classList.remove("hover");
                    }
                }
            }, { passive: true }); // Add { passive: true } here as well
        });
    });

    const mediaQuery = window.matchMedia("(max-width: 991px)");
    document.querySelectorAll(".c-menu-dropdown").forEach((item)=>{
        item.addEventListener("click", function(e) {
            if (mediaQuery.matches) {
                let siblings = Array.from(item.parentNode.children);
                siblings.forEach((sibling)=>{
                    if (sibling.classList.contains("c-submenu-2")) {
                        sibling.classList.toggle("mb-open");
                    }
                });
            }
        });
    });</script> </body></html>


