<script type="text/javascript">  
   function getSectionList(){
     var list = "";
     var index = 1;
     var item = virtualcmsPageInfo.sectionTreeWt[index];
     while(item){
       if(list!=""){
        list = list + ", ";
       }
       list = list + item;
       index++;
       item = virtualcmsPageInfo.sectionTreeWt[index];
     }
     return list;
