<script>
  // Update site specific details
  $(function(){

    var test_mode=getUrlVars()['test_mode'];
    
    if(!test_mode){
      // If test mode not specified in url, then check cookie.
      test_mode=getCookie('test_mode');
    } else {
      setCookie("test_mode",test_mode, 0);    
    }

    if(test_mode=="1"){      
      $("#test_mode").html("1");
      $('.test_mode').show();
    } else {      
      test_mode="0";
      $("#test_mode").html("0");
    }

    var channel_name=$("#channel_name").html();

    // Hide epaper options if corklive or waterfordlive or carlowlive
    if((channel_name=="corklive")||(channel_name=="waterfordlive")||(channel_name=="carlowlive")){
      $(".epaper").hide();
    } else {
      $(".epaper").show();
    }
    
    // Read a page's GET URL variables and return them as an associative array.
    function getUrlVars()    {
      var vars = [], hash;
      var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
      for(var i = 0; i < hashes.length; i++)
      {
          hash = hashes[i].split('=');
          vars.push(hash[0]);
          vars[hash[0]] = hash[1];
      }
      return vars;
    }

    // Update story container
    if(($("#story_contact_container").length)&&($("#story_contact").length)){
      $("#story_contact_container").html($("#story_contact").html());
    }
    
    // Update domain name
    $('.domain').html($('#domain').html());

    // Update site name
    $('.site_name').html($('#site_name .text_edit .text').html());
    
    function add_output($value){
      console.log($value);
      //$('#test_output').html($('#test_output').html() + '<br />' + $value);      
    }

    var site_user_id=getCookie('site_user_id');
    var ad_blocker_detected_id=getCookie('ad_blocker_detected_id');
    
    function newId(){
      // Set Site User ID
      var MyDate = new Date();
      var MyDateString;
      MyDateString = MyDate.getFullYear() + '-' + ('0' + (MyDate.getMonth()+1)).slice(-2) + '-' + ('0' + MyDate.getDate()).slice(-2) 
                      + '_' + ('0' + MyDate.getHours()).slice(-2) + ('0' + MyDate.getMinutes()).slice(-2) + ('0' + MyDate.getSeconds()).slice(-2)  ;
      return MyDateString + '_' + Math.floor(Math.random() * 10000)+1;
    }

    if(!site_user_id && ad_blocker_detected_id){
      site_user_id=ad_blocker_detected_id;
      // Set Site User ID to be the old ad_blocker_detected_id.
      setCookie('site_user_id', site_user_id, 365*20);
      log("new_site_user_id_from_old_id");  
    }
    if(!site_user_id){
      // Set Site User ID
      site_user_id=newId();        
      setCookie('site_user_id', site_user_id, 365*20);
      log("new_site_user_id");
    }
    if(ad_blocker_detected_id){
      // Remove old cookie
      setCookie('ad_blocker_detected_id', ad_blocker_detected_id, -365*20);
      log("delete_old_cookie");
    }
    detectAdBlocker();


    function log(note){
      add_output(note);

      // 18/01/2022: Disabled logging for now.
      //$.ajax({url: "https://iconicnews.ie/link/?note=" + note + ";referrer=" + document.referrer + "&source=site_user_id_" + site_user_id + "&url=" + window.location.href, success: function(result){
      //  add_output('ajax returned=' + result + '.');
      //}});
    }

    function detectAdBlocker(){
      if($("#ad-iqdyClY6VoXQPZJ0ntq2").length){
        log("adblocker_off");
      } else {
        log("adblocker_on");
        if(getCookie('ad_blocker_detected')=="1"){
          add_output('Ad Blocker Detected dialog already shown before.');
        } else {
          log("adblocker_message");
          show_ad_blocker_detected_modal();
        }
      }
    }

    $("#show_ad_blocker_detected_modal_button").click(function(){
      show_ad_blocker_detected_modal();
    });
    
    function show_ad_blocker_detected_modal(){
      $('#ad_blocker_detected_modal').modal({
        backdrop: 'static',
        keyboard: false
      });
      setCookie('ad_blocker_detected','1', 1);
    }

    // Button actions
    $("#maybe_later_button").click(function(){
      log("maybe_later_button");
    });

    $("#contribute_button").click(function(){
      log("contribute_button");
      window.location.href="https://www.northdevongazette.co.uk/contribute";
    });

    $("#newsletter_button").click(function(){
      log("newsletter_button");
      window.location.href="/register";
    });

    
    function setCookie(name, value, days) {
        var date = new Date(),
        expires = 'expires=';
        if(days!=0){
          date.setTime(date.getTime() + 1000*60*60*24*days);
          expires += date.toGMTString();
          add_output('Cookie expires:' + expires + '.');
        }
        document.cookie = name + '=' + value + '; ' + expires + '; path=/;';
        //document.cookie = name + '=' + value + '; path=/;';
    }

    function getCookie(name) {
        var allCookies = document.cookie.split(';'),
            cookieCounter = 0,
            currentCookie = '';
        for (cookieCounter = 0; cookieCounter < allCookies.length; cookieCounter++) {
            currentCookie = allCookies[cookieCounter];
            while (currentCookie.charAt(0) === ' ') {
                currentCookie = currentCookie.substring(1, currentCookie.length);
            }
            if (currentCookie.indexOf(name + '=') === 0) {
                return currentCookie.substring(name.length + 1, currentCookie.length);
            }
        }
        return false;
    }
  });
</script>

