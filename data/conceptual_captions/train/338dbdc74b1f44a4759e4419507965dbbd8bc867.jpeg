                   $(this).attr({
                     'aria-current': 'page',
                   });
                 }

                 $(this).attr({
                   'tabindex': '0',
                   'focusable': 'true',
                   'role': 'link',
                   'aria-controls': 'vt_search_results',
                 }).wrap('<li role="presentation" class="vt-search-pagination-page"></li>');
               });

               $('.vt-search-pagination-page:last-of-type > .gsc-cursor-page').on('keydown', function(e) {
                 if ( e.which == 9 )  {
           				e.preventDefault();
           				$('.vt-search-toggle').focus();
           			}
               });
            }
          };

            window.__gcse || (window.__gcse = {});
            window.__gcse.searchCallbacks = {
           		 web: {
           				 starting: mySearchStartingCallback,
           				 ready: myResultsReadyCallback,
           				 rendered: myWebResultsRenderedCallback,
           		 },
            };

            // END GCSE
          </script>
