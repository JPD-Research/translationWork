    <script type="text/javascript">
    function hasClass(element, cls) {
            return (' ' + element.className + ' ').indexOf(' ' + cls + ' ') > -1;
        }

        function handleToggleHamburgerMenu() {
            const primaryNav = document.getElementById('ibn-nav');
            const dropdownMenu = document.getElementById('navbarSupportedContent');
            const secondaryNav = document.getElementById('ibu-nav-secondary');

            let header = document.getElementById('home-nav-bar');
            let headerHeight = header != null ? getComputedStyle(header).height.split('px')[0] : '';
            let scrollValue = window.scrollY;

            if (!hasClass(dropdownMenu, 'show')) {
                secondaryNav!= null ? secondaryNav.style.visibility = 'hidden' : '';;
                primaryNav.classList.add("is-fixed");
            } else if (scrollValue < headerHeight){
                secondaryNav!= null ? secondaryNav.style.visibility = 'visible' : '';
                primaryNav.classList.remove("is-fixed");
            }
        }
</script>
    <script type="text/javascript">
        function handleExpandSubscribeFormBtn() {
            document.querySelector("#news-letter-area .expandSubscribeFormBtn").classList.add("d-none");
            document.querySelector("#news-letter-area .subscription-forms-container").classList.add("d-block");
        }

        function recaptchaNewsSubscribeFormSuccessCallBack() {
            // Reset custom form components
            document.querySelector("#news-letter-area .buyerTypeDropDown button").classList.add("placeholder");
            document.querySelector("#news-letter-area .buyerTypeDropDown button span").textContent = "I am a...";

            document.querySelector("#news-letter-area .statesDropDown button").classList.add("placeholder");
            document.querySelector("#news-letter-area .statesDropDown button span").textContent = "In state...";

            document.querySelectorAll("#news-letter-area .dropdown-menu input").forEach((input) => {
                input.value = null;
            });

            document.querySelector("#news-letter-area .successfulSubscription").classList.add("d-block");
            document.querySelector("#news-letter-area .successfulSubscription").textContent = "You have successfully subscribed.";

            document.querySelector("#news-letter-area .field-email input").classList.remove("error");

            window._vis_opt_queue = window._vis_opt_queue || [];
            window._vis_opt_queue.push(function() {_vis_opt_goal_conversion(207);});

            window.VWO = window.VWO || [];
            window.VWO.push(['track.goalConversion',7]);
        }

        function handleFormErrorCallback() {
            if(!document.querySelector('#news-letter-area .dropdown-menu input[name="state"]').value)
                document.querySelector("#news-letter-area .statesDropDown button").classList.add("error");
            if(!document.querySelector('#news-letter-area .dropdown-menu input[name="buyer_type"]').value)
                document.querySelector("#news-letter-area .buyerTypeDropDown button").classList.add("error");
            document.querySelector("#news-letter-area .field-email input").classList.add("error");
        }

        function recaptchaNewsLetterSubscribeSectionFormSubmitAjax() {
            let recaptchaHtmlId = "newsLetterSubscribeSectionForm-recaptcha";

            
            ajaxFormSubmit(
                "https://www.ibuynew.com.au/subscribe",
                document.getElementById("newsLetterSubscribeSectionForm"),
                "",
                "newsLetterSubscribeSectionForm-error",
                "newsLetterSubscribeSectionForm-input",
                "newsLetterSubscribeSectionForm-reset",
                recaptchaHtmlId,
                handleFormErrorCallback,
                recaptchaNewsSubscribeFormSuccessCallBack
            );
        }
