<script type="text/javascript">

  const footerMenu = () =>  {
    const linkGroupsNodeList = document.querySelectorAll('.footer-links-group');
    const linkGroupsArr = Array.prototype.slice.call(linkGroupsNodeList);

    for (var i = 0, len = linkGroupsArr.length, h5, ul; i < len; i++) {
      h5 = linkGroupsArr[i].querySelectorAll('a.h5');
      ul = linkGroupsArr[i].querySelectorAll('ul');
      h5[0].addEventListener('click', function(mouseEvent) {
          mouseEvent.preventDefault();
          let amClosing = mouseEvent.currentTarget.classList.contains('open');
          const nodeToCloseNodeList = document.querySelectorAll('.footer-links-group a.h5.open');
          const nodeToCloseArr = Array.prototype.slice.call(nodeToCloseNodeList);

          for (var i2 = 0, len2 = nodeToCloseArr.length; i2 < len2; i2++) {
            nodeToCloseArr[i2].classList.remove('open');
            nodeToCloseArr[i2].setAttribute('aria-expanded', false);
            const closeListId = nodeToCloseArr[i2].getAttribute('data-rel');
            document.getElementById(closeListId).classList.add('collapsed');
          } 
          if (!amClosing) {
            mouseEvent.target.classList.add('open');
            mouseEvent.currentTarget.classList.add('open');
            
            document.getElementById(mouseEvent.currentTarget.getAttribute('data-rel')).classList.remove('collapsed');
          
            if (nodeToCloseArr[i2]) {
              nodeToCloseArr[i2].setAttribute('aria-expanded', true);
            }
          }
      });
    }
  };
  const mobileMenu = () => {
