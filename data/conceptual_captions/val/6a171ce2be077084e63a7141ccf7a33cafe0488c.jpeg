      const links = document.querySelectorAll('a');
      const videoCont = document.getElementById('video-cont');

      videoCont.classList.toggle('hidden');
      for (let i = 0; i < links.length; i++) {
        const link = links[i];

        if (!link.classList.contains('mobile-nav-item')) {
          isOpen ? link.setAttribute('tabindex', '-1'): link.setAttribute('tabindex', '0');
        }
      }
    }
    
    // debounce function added to improve performance of 'resize' event listener

    const debounce = function(fn) {
      let timer;

      return () => {
        if (timer) return;
        timer = window.requestAnimationFrame(() => {
          fn();
          timer = undefined;
        });
      }
    }

    // handleResize closes the mobile nav at desktop width (>1124px)

    const handleResize = function(e) {
      const isOpen = mobileNav.classList.contains('menu-open');
    
      if (isOpen && this.innerWidth > 1124) {
        mobileNav.classList.remove('menu-open');
        html.classList.remove('menu-open');
        body.classList.remove('menu-open');
        mobileNavClose.classList.remove('menu-open');
        mobileNavOptions.classList.remove('menu-open');
        toggleTabIdx(false);
      }
    }

    menuIcon.addEventListener('click', function(e) {
      mobileNav.classList.add('menu-open');
      html.classList.add('menu-open');
      body.classList.add('menu-open');
      mobileNavClose.classList.add('menu-open');
      mobileNavOptions.classList.add('menu-open');
      toggleTabIdx(true);
    });
    menuIcon.addEventListener('keypress', function(e) {
      if (e.key === 'Enter') {
        mobileNav.classList.add('menu-open');
        html.classList.add('menu-open');
        body.classList.add('menu-open');
        mobileNavClose.classList.add('menu-open');
        mobileNavOptions.classList.add('menu-open');
        menuClose.focus();
        toggleTabIdx(true);
      }
    });
    menuClose.addEventListener('click', function(e) {
      mobileNav.classList.remove('menu-open');
      html.classList.remove('menu-open');
      body.classList.remove('menu-open');
      mobileNavClose.classList.remove('menu-open');
      mobileNavOptions.classList.remove('menu-open');
      toggleTabIdx(false);
    });
    menuClose.addEventListener('keypress', function(e) {
      if (e.key === 'Enter') {
        mobileNav.classList.remove('menu-open');
        html.classList.remove('menu-open');
        body.classList.remove('menu-open');
        mobileNavClose.classList.remove('menu-open');
        mobileNavOptions.classList.remove('menu-open');
        toggleTabIdx(false);
      }
    });
    window.addEventListener('resize', debounce(handleResize));
    window.addEventListener('click', function(e) {
      if (this.innerWidth > 1124 || this.innerWidth < 752) return;
      if (e.target === menuIcon || e.target.parentElement === menuIcon) return;
      
      const isOpen = mobileNav.classList.contains('menu-open');
      const isOutside = !e.target.closest('.mobile-menu');

      if (isOpen && isOutside) {
        mobileNav.classList.remove('menu-open');
        html.classList.remove('menu-open');
        body.classList.remove('menu-open');
        mobileNavClose.classList.remove('menu-open');
        mobileNavOptions.classList.remove('menu-open');
        toggleTabIdx(false);
      }
    });
  };
  
  const isLandingPage = !document.querySelector('.core-footer');
  document.addEventListener('DOMContentLoaded', function(event) {
    footerMenu();
    if (isLandingPage) {
      mobileMenu();
    }
  });

</script>
