		<script type="text/javascript">
		;(function($){

			var $map, map, lat, lng, marker, marker_postition, infowindow, infowindow_content, geocoder, geocode_address;

			initMap = function(){

				$map = $('#map_panel');

				if( ! $map.length ) return false;

				lat = $map.attr( 'data-latitude' );
				lng = $map.attr( 'data-longitude' );

				if( lat && lng ){
					marker_postition = new google.maps.LatLng( lat,lng );

					buildMap();
				} else {

					/**
					 * If we do not have the latitude and longitude
					 * then get them via geocoding
					 */
					geocoder = new google.maps.Geocoder();

					geocode_address = $map.attr( 'data-street' ) + ' '
									+ $map.attr( 'data-town' ) + ' '
									+ $map.attr( 'data-county' ) + ' '
									+ $map.attr( 'data-postcode' ) + ' '
									+ $map.attr( 'data-country' );

					geocoder.geocode( { 'address' : geocode_address }, function( results, status ){

						if( 'OK' == status ){

							marker_postition = results[0].geometry.location;
							buildMap();
						} else {

							if( window.console ){
								console.log( 'Cannot build map because of reasons.' );
								console.log( status );
							}
						}
					});
				}
			}

			buildMap = function(){

				infowindow_content = '<div id="infowindow_content">';
				infowindow_content+= 	'<h1 style="font-size:2em;">' + $map.attr( 'data-title' ) + '</h1>';
				infowindow_content+= 	'<div class="row">';
				infowindow_content+= 		'<ul>'
				infowindow_content+= 			'<li style="font-size:1em;">' + $map.attr( 'data-street' ) + '</li>';
				infowindow_content+= 			'<li style="font-size:1em;">' + $map.attr( 'data-town' ) + '</li>';
				infowindow_content+= 			'<li style="font-size:1em;">' + $map.attr( 'data-county' ) + '</li>';
				infowindow_content+= 			'<li style="font-size:1em;">' + $map.attr( 'data-postcode' ) + '</li>';
				infowindow_content+= 			'<li style="font-size:1em;">' + $map.attr( 'data-country' ) + '</li>';
				infowindow_content+= 		'</ul>';
				infowindow_content+= 	'</div>';
				infowindow_content+= '</div>';

				infowindow = new google.maps.InfoWindow({
					content: infowindow_content
				});

				map = new google.maps.Map( document.getElementById( 'map_panel' ), {
					zoom: 14,
					center : marker_postition,
					scrollwheel: false,
					mapTypeControl: true,
			        mapTypeControlOptions: {
			            style: google.maps.MapTypeControlStyle.HORIZONTAL_BAR,
			            position: google.maps.ControlPosition.TOP_CENTER
			        },
			        zoomControl: true,
			        zoomControlOptions: {
		            	position: google.maps.ControlPosition.LEFT_CENTER
		            },
			        scaleControl: true,
			        streetViewControl: true,
			        streetViewControlOptions: {
			            position: google.maps.ControlPosition.LEFT_TOP
			        },
			        fullscreenControl: true
				} );

				marker = new google.maps.Marker({
					position: marker_postition,
					map: map
				});

				google.maps.event.addListener( marker, 'click', function( evt ){

					infowindow.open( map, marker );
				});
			}
		})( jQuery )
		</script>

