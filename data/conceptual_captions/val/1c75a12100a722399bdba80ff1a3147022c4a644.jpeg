  <script>
    $(document).ready(function() {
      footerReform();

      function footerReform() {
        if ($(".lottog-footer").width() < 900) {
          $(".lottog-footer .site-map > li > span").unbind("click");
          $(".lottog-footer .site-map > li > ul").slideUp();
          $(".lottog-footer .site-map > li > span").on("click", function() {
            if ($(this).parent('li').hasClass("expanded")) {
              $(this).parent('li').removeClass("expanded");
              $(this).next('ul').slideUp();
            } else {
              $(this).parent('li').siblings().removeClass("expanded");
              $(this).parent('li').addClass("expanded");
              $(this).parent('li').siblings().find("ul").slideUp();
              $(this).next('ul').slideDown();
            }
          });
        } else {
          $(".site-map > li > span").unbind("click");
          $(".site-map > li > ul").slideDown();
        }
      }
      $(window).resize(function() {
        footerReform();
      });
    });
  </script>

