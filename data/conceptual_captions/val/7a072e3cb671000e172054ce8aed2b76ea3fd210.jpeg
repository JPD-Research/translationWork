            var source = 'events';
            var currentDate = $(this).prop('id');
            $.ajax({
                url: "https://www.27east.com/wp-content/themes/27East/template-filter-home-new.php",
                data: {
                    day: currentDate,
                    source: source
                },
                dataType: "html",
                type: 'post',
                success: function(filteredPosts) {
                    $('#event_list').show();
                    $('#event_list').html(filteredPosts);
                    $('#svgLoader').hide();
                },
                error: function(error) {
                    console.log(error)
                }
            })
        });
    }
