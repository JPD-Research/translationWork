/* remove extra text after flash 90 captions */
function removeCaptionText() {
  // Get all the figcaption elements on the page
  var figcaptions = document.getElementsByTagName('figcaption');
  // Loop through each figcaption element
  for (var i = 0; i < figcaptions.length; i++) {
    // Get the caption text of the figcaption
    var caption = figcaptions[i].textContent;
    // Find the position of the word "Flash90" in the caption
    var pos = caption.indexOf('Flash90');
    // If the word is found, remove everything after it
    if (pos !== -1) {
      caption = caption.slice(0, pos + 7);
      // Set the modified caption as the new text content of the figcaption
      figcaptions[i].textContent = caption;
    }
  }
}

// Call the function when the page loads
window.addEventListener('load', removeCaptionText);

// Call the function when a new post is loaded
