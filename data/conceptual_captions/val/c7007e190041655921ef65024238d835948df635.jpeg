<style>/* defining variables */
.viafoura {
    --primary-font: "Source Sans Pro", sans-serif;
    --accent-color:  var(--color-primary);
    --accent-color-light: var(--color-primary);
    --accent-color-dark: var(--color-button-hover-bg);
    --accent-color-90: rgba(0, 125, 179, 0.9);
    --accent-color-20: rgba(0, 125, 179, 0.2);
    --accent-color-10: rgba(0, 125, 179, 0.1);
    --accent-color-5: rgba(0, 125, 179, 0.5);
}

/* bell & tray styling */
.viafoura .vf-tray-trigger:hover,
.viafoura.header-bell:hover { color: var(--accent-color) !important; }

.Page-header-bar .viafoura button, .viafoura .vf-tray-trigger:focus { color: var(--color-primary-text) !important; }

.viafoura.header-bell { 
    visibility: hidden;
    position: relative;
    color: var(--color-primary-text);
    width: 40px;
    height: 40px;
}

.viafoura.header-bell .vf-icon_bell_icon { display: none;} 

.viafoura.header-bell:has(button) { visibility: visible; }
/* needed to target just firefox */
@supports (-moz-transform: translate(0, 0)) {
  .viafoura.header-bell { visibility: visible; }
}

.viafoura.header-bell .vf-fa-outlineBell {
    width: 23px;
    height: 23px;
    position: absolute;
    top: 8.5px;
    left: 8.5px;
}

.viafoura .vf-accent-border-colour, 
.viafoura .vf-expanding-hover-light::before { 
    border-color: var(--accent-color) !important;
}
.viafoura .vf-tray .vf-settings .vf-client-settings-button-logout {
    display: none;
}

/* needed for tray to show above Page-header */
.viafoura .vf-tray-slide-wrapper {
    z-index: 100000009 !important;
}

.Page-header-bar .viafoura {
    height: 40px;
    overflow: hidden;
}

/* share bar styles */
.viafoura .vf-share-bar {
    padding-top: 0px !important;
    position: relative !important;
    display: inline-block !important;
    letter-spacing: .5px !important;
    border: none !important;
    width: 100% !important;
}
.viafoura .vf-share-bar .vf-share-bar-service-name {
    color: #666666 !important;
    font-size: 0.875rem !important;
