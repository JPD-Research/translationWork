    <script type='text/javascript'>
        function ensureUserIsSetChartbeat () {
          return new Promise(waitForUser)

          function waitForUser (resolve) {
            if (window.user && window.user.status) resolve(window.user.status)
            else if (window.location.host.indexOf("therinklive") > -1) resolve()
            else if (window.location.host.indexOf("siouxfallslive") > -1) resolve()
            else if (window.location.host.indexOf("stcloudlive") > -1) resolve()
            else setTimeout(waitForUser.bind(this, resolve), 100)
          }
        }

        (function () {
            /** CONFIGURATION START **/
            var _sf_async_config = window._sf_async_config = (window._sf_async_config || {});
            _sf_async_config.uid = 63431;
            _sf_async_config.domain = 'inforum.com';
            _sf_async_config.flickerControl = false;
            _sf_async_config.useCanonical = true;
            _sf_async_config.useCanonicalDomain = true;
            _sf_async_config.sections = '';
            _sf_async_config.authors = '';
            ensureUserIsSetChartbeat().then(() => {
                if (typeof user !== 'undefined') {
                  if (user.status !== "anonymous") {
                      _sf_async_config.idSync = _sf_async_config.idSync || {};
                      _sf_async_config.idSync.l = window.getForumCookie('auth0_sub');
                      }
                  var _cbq = window._cbq = (window._cbq || []);
                  if (user.status == "anonymous") {
                      _cbq.push(['_acct', 'anon']);
                  }
                  else if (user.status == "registered") {
                      _cbq.push(['_acct', 'lgdin']);
                  }
                  else {
                      _cbq.push(['_acct', 'paid']);
                  }
                }

                /** CONFIGURATION END **/
                function loadChartbeat() {
                    var e = document.createElement('script');
                    var n = document.getElementsByTagName('script')[0];
                    e.type = 'text/javascript';
                    e.async = true;
                    e.src = '//static.chartbeat.com/js/chartbeat.js';
                    n.parentNode.insertBefore(e, n);
                }

                loadChartbeat();
            });
        })();
    </script>

